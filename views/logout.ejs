<!DOCTYPE html>
  <head>
    <%- include('imports/head') %>
    <title>Concredito Login</title>
  </head>
  <body style="background-image: linear-gradient(to bottom right, rgb(222, 0, 255), rgb(0, 255, 255)); width: 100vw; height: 100vh; max-width: 100%; max-height: 100%; overflow: hidden">
    <div style="background-color: white; height: 50vh; width: 30vw; max-height: 50vh; max-width: 30vw; overflow: hidden; margin-top: 25vh; margin-left: 35vw; border-radius: 1vh; box-shadow: 0.4vh 0.4vh 1.5vh 1vh #00000080; text-align: center;">
    </div>
  </body>
  <script>
    function logout() {
      localStorage.setItem('user', false)
      localStorage.setItem('password', false)
      location.href = `/`
    }(function () { logout() })();
  </script>
</html>